package com.bootcamp.corejava.introduction.lab3;


import static java.lang.System.out;

/**
 * @author Jagan Reddy
 * This class prints the Fight song which has repeated lines
 */
public class FightSong {

	/*
	 * This method print some lines song 
	 */
	static void songSnippet1() {
		out.println("Go, team, go!\nYou can do it.");
	}
	
	/*
	 * This method print some lines of song
	 */
	static void songSnippet2() {
		out.println("You're the best,\nIn the West.");
	}
	
	/*
	 * This method print Space
	 */
	static void printSpace() {
		out.println();
	}

	public static void main(String[] args) {
		
		songSnippet1();
		printSpace();
		songSnippet1();
		songSnippet2();
		songSnippet1();
		printSpace();
		songSnippet1();
		songSnippet2();
		songSnippet1();
		printSpace();
		songSnippet1();

	}

}
