package com.bootcamp.corejava.flowcontrols.lab4;


/**
 * @author Jagan Reddy
 * This class has functionality to print the table
 */
public class Table {
	
	/**
	 * Prints the table for n = 10 n=100  n=1000
	 */
	public static void printTable(){
		int n = 10;
		System.out.println(n+"*N" + "  "+n*n+"*N" +"  "+n*n*n+"*N");	
		System.out.println("----------------------");
		for (int i = 1; i <= 5; i++) {
			System.out.println(n*i + "     "+n*n*i +"     "+n*n*n*i);			
		}
	}
	
	public static void main(String[] args) {
		printTable();
	}

}
