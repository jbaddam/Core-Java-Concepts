package com.bootcamp.corejava.oop.classes.medium.lab3;


/**
 * @author Jagan Reddy
 * This class has a functionality to return date 
 */
public class MyDate {
	
	private int month;
	private int day;
	private int year;
	
	
	public MyDate(int month, int day, int year) {
		super();
		this.month = month;
		this.day = day;
		this.year = year;
	}


	public int getMonth() {
		return month;
	}


	public void setMonth(int month) {
		this.month = month;
	}


	public int getDay() {
		return day;
	}


	public void setDay(int day) {
		this.day = day;
	}


	public int getYear() {
		return year;
	}


	public void setYear(int year) {
		this.year = year;
	}
	
	/**Method to return date
	 * @return
	 */
	public String displayDate() {
		return ""+getMonth()+"/"+getDay()+"/"+getYear();
	}
	
	
	

}
