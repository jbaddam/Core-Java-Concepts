package com.bootcamp.corejava.oop.interfaces.lab3;


/**
 * @author Jagan Reddy
 * Lion class implementing LandMammal interface
 */
public class Lion implements LandMammal {

	@Override
	public void walk() {
		System.out.println("Lion Walking");
		
	}

	@Override
	public void run() {
		System.out.println("Lion Running");		
	}

	@Override
	public void crawl() {
		System.out.println("Lion Crawling");	
		
	}

	@Override
	public void eat() {
		System.out.println("Lion Eating");	
		
	}
	
	public void roar(){
		System.out.println("Lion Roaring");
	}

	

}
